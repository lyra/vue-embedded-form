{
  "name": "@lyracom/vue-embedded-form",
  "version": "0.0.8",
  "description": "Vue Lyra embedded form",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "coveragePathIgnorePatterns": [
      "/tests/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!vue-awesome)"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyra/vue-embedded-form.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "embedded",
    "form",
    "lyra"
  ],
  "author": "Lyra Network",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lyra/vue-embedded-form/issues"
  },
  "homepage": "https://github.com/lyra/vue-embedded-form#readme",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "jsdom": "^13.0.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "vue": "^2.5.17",
    "vue-jest": "^3.0.0",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "@vue/cli-plugin-unit-jest": "^3.1.1",
    "vue-router": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9",
    "ie 10"
  ],
  "dependencies": {
  }
}
